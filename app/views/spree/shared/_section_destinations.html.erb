<div class="section container">
   <h2><%= t(:popular_destinations) %> <%= image_tag('icon/star_blue_32x32.png', class: 'mytrp_icon') %></h2>

   <div class="row image-box style3">
     <div class="col-sms-6 col-sm-6 col-md-3">
       <article class="box">
         <figure class="animated" data-animation-type="fadeInDown" data-animation-delay="0">
           <a href="ajax/cruise-slideshow-popup.html" class="hover-effect popup-gallery"> <%= image_tag('https://s3-us-west-2.amazonaws.com/webtriptocuba/habana.jpg', size: "270x160", alt: "La Habana") %></a>
         </figure>
         <div class="details text-center">
           <h4 class="box-title"><%= t(:destinohabana) %></h4>

           <div data-placement="left" data-toggle="tooltip" title="4 stars" class="five-stars-container">
             <span style="width: 80%;" class="five-stars"></span>
           </div>
           <%= t(:description_destino_habana_html) %>
           <a class="button" href="cruise-detailed.html"><%= t(:see_all) %></a>
         </div>
       </article>
     </div>
     <div class="col-sms-6 col-sm-6 col-md-3">
       <article class="box">
         <figure class="animated" data-animation-type="fadeInDown" data-animation-delay="0.4">
