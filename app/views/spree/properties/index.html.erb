<% content_for :pagetitle do %>
    <%= render partial: 'spree/properties/pagetitle' %>
<% end %>

<% content_for :travel_search_box do %>
    <%= render :partial => 'spree/properties/search_box', :locals => {id: 'properties', active: true} %>
<% end %>

<%= render partial: 'spree/properties/search_result' %>
<%= render partial: 'spree/properties/list' %>
<%= render partial: 'spree/properties/grid' %>
<%= render partial: 'spree/properties/block' %>
<%= render partial: 'spree/properties/details' %>

<script type="text/javascript">
    tjq(document).ready(function () {
        if (tjq('#search-result-items').length == 0) {
            tjq('section#content div#main div.row').html('');
            tjq('section#content div#main div.row').mustache('search-result-section');
        }
        tjq('#search-result-items').renderProducts('houses');
    })
</script>

