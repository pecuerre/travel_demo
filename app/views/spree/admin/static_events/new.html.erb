<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<% content_for :page_title do %>
  <%= t(:new_static_event) %>
<% end %>

<% content_for :page_actions do %>
  <li><%= link_to_with_icon 'arrow-left', t(:back_to_static_events_list), admin_static_events_path, :class => 'button' %></li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @static_events } %>

<%= form_tag admin_static_events_path do -%>
  <fieldset class="no-border-top">
    <%= render :partial => 'form' %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end -%>
