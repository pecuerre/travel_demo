class Products.Base
  constructor: (@item) ->
    prices = @item.prices ? {}
    @offer_price = Math.floor(prices[@item.api.name] ? 0 )

  booking_uri: () ->
    if @item.booking_uri != undefined
      @bu_value()
    else
      ""

  same_booking_uri: () ->
    if @item.same_booking_uri != undefined
      @same_bu_value()
    else
      ""

  _eval: (name) ->
    if @.hasOwnProperty(name)
      @[name]
    else
      @[name]()

  to_dom: ->
    if @dom? then @dom else @dom = $(@template())

